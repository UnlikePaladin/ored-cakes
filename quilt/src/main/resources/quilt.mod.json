{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "ncake",
    "version": "${version}",
    "name": "Netherite Cake Mod",
    "description": "Brings Netherite Cake to your reality.",
    "authors": ["UnlikePaladin"],
    "contact": {
      "homepage": "https://www.curseforge.com/minecraft/mc-mods/netherite-cake",
      "sources": "https://github.com/UnlikePaladin/netherite-cake"
    },
    "license": "LGPL-3.0-only",
    "icon": "assets/ncake/ncake_logo_square.png",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "environment": "*",
    "entrypoints": {
      "init": [
        "com.unlikepaladin.ncake.quilt.NCakeModQuilt"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "version": "*"
      },
      {
        "id": "quilt_base",
        "version": "*"
      },
      {
        "id": "minecraft",
        "version": ">=1.18.2"
      }
    ]
  }
}